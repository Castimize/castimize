{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "codenco-dev/nova-grid-system": "^2.0",
        "darkaonline/l5-swagger": "^8.6",
        "devloops/phone-number": "^1.0",
        "digital-creative/resource-navigation-tab": "^2.1",
        "guzzlehttp/guzzle": "^7.9",
        "jeffbeltran/sanctum-tokens": "^2.1",
        "kaiserkiwi/nova-queue-management": "^1.2",
        "laravel/framework": "^11.9",
        "laravel/nova": "^4.0",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.9",
        "league/flysystem-aws-s3-v3": "^3.0",
        "nsavinov/nova-percent-field": "^1.0",
        "picqer/exact-php-client": "^4.4",
        "predis/predis": "^2.2",
        "ranium/laravel-seedonce": "^1.6",
        "saumini/count": "^1.0",
        "sentry/sentry-laravel": "^4.8",
        "sereny/nova-permissions": "^1.6",
        "shippo/shippo-php": "1.*",
        "spatie/laravel-permission": "^6.9",
        "stripe/stripe-php": "^15.8",
        "venturecraft/revisionable": "^1.41",
        "vyuldashev/nova-money-field": "^2.0",
        "wamesk/laravel-nova-tel-input": "^1.0",
        "wildside/userstamps": "^2.4"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "phpunit/phpunit": "^11.0.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "repositories": {
        "nova": {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    }
}
